[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
[gpg]
	program = gpg
[pull]
	rebase = false
[core]
    pager = delta
[delta]
    side-by-side = true
[interactive]
    diffFilter = delta --color-only
[user]
	name = "{{ .name }}"
	email = "{{ .email }}"
